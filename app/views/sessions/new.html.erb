<!-- sessions.new.html.erb -->

<h1>Log in</h1>
<% if flash[:notice] %>
  <div class="flash notice">
    <span class=alert> 
        <%= flash[:notice] %> 
    </span>
  </div>
<% end %>
<br>
<br>
   <%= form_for @user , url: {action: :create} do |f| %>

      <%= f.label :username %>
      <%= f.text_field :username %>
      <br>
      <br>
      <%= f.label :password %>
      <%= f.text_field :password %>

      <%= f.submit "login" %>
 <% end %>












