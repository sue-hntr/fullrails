<!DOCTYPE html>
<html>
<head>
  <title>Wk05FullRails</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:notice] %>
	<div class="flash notice">
		<span class=alert> 
				<%= flash[:notice] %> 
		</span>
	</div>
<% end %>
<% if flash[:alert] %>
	<div class="flash notice">
		<span class=alert> 
				<%= flash[:alert] %> 
		</span>
	</div>
<% end %>




<%= yield %>

</body>
</html>
